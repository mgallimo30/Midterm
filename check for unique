
// Verify that all elements of fullListofNames are antiques

	private static boolean checkfullListofNames() {
		for(int i = 0;i< fullListofNames.size();i++)
		{

			for(int j = i+1;j< fullListofNames.size();j++) 
			{
				if(j != i && fullListofNames.get(i).compareToIgnoreCase(fullListofNames.get(j)) == 0) {
					return false;
				}
			}
		}
		return true;		
	}
